    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router": "7.0.1-pre.0"
  },
  "devDependencies": {
    "@react-router/dev": "7.0.1-pre.0",
    "@types/node": "^20",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "autoprefixer": "^10.4.20",
    "postcss": "^8.4.49",
    "tailwindcss": "^3.4.15",
    "typescript": "^5.6.3",
